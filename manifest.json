{
    "manifest_version": 3,
    "name": "LyncX",
    "version": "1.0.0",
    "key": "",
    "description": "See Your History Like Never Before",
    "permissions": [
        "identity",
        "tabs",
        "storage",
        "activeTab",
        "webNavigation",
        "idle",
        "windows",
        "declarativeNetRequest",
        "scripting",
        "downloads",
        "contextMenus",
        "alarms",
        "notifications",
        "identity.email",
        "clipboardRead",
        "clipboardWrite"
    ],
    "host_permissions": [
        "<all_urls>",
        "https://www.googleapis.com/",
        "https://*.google.com/"
    ],
    "oauth2": {
        "client_id": "606602321768-7dh5u6k0saap2lbd223pjlqqk6sflhnr.apps.googleusercontent.com",
        "scopes": [
            "openid",
            "email",
            "profile",
            "https://www.googleapis.com/auth/calendar.readonly",
            "https://www.googleapis.com/auth/calendar.events",
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    },
    "action": {
        "default_popup": "src/popup/popup.html"
    },
    "background": {
        "service_worker": "src/data/background.ts"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["src/services/contentScript.ts"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "src/settings/settings.html",
                "waterfall.html",
                "src/graph/graph.html",
                "src/dashboard/dashboard.html",
                "src/main/main.html",
                "src/landing/landing.html",
                "src/assets/fonts/*.ttf",
                "src/assets/leaves.png",
                "src/auth/login.html"
            ],
            "matches": ["<all_urls>"]
        }
    ]
}
